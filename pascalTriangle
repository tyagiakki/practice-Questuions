package practice_question;

public class pascalTriangle {
	public static int triangle(int n,int m) {
	int  res=1;
		for(int i=0;i<m;i++) {
			res=res*(n-i);
			res=res/(i+1);	
		}
		return res;
	}
	public static void main(String[] args) {
		int row=5;
		int col=3;
		triangle(row,col);
		System.out.println(triangle(row,col));
	}
}
//OUTPUT:- 10
