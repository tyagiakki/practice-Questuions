package practice_Questions;

import java.util.Scanner;

public class primeNumber {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=2;i<n;i++) {
			if(n%i==0) {
				System.out.println("no");
				break;
			}
			else
			{
				System.out.println("yes");
				break;
			}
		}
	}
}
